{
  "name": "OL",
  "version": "1.0.0",
  "description": "react, typescript",
  "main": "index.js",
  "scripts": {
    "clean-logs": "rm -fr npm-debug.log.*",
    "clean": "rm -fr dist && rm -fr build/index.html && rm -fr build/bundle.js && npm run clean-logs",
    "raze": "npm run clean && rm -fr node_modules",
    "code-generation.README": "in the past the below used to be `npm bin`/ts-interface-builder .. but `npm run` was changed to npx to allow building on Windows",
    "code-generation": "npx ts-interface-builder ./src/models/backend-applicant-identity.ts ./src/models/backend-phone.ts ./src/models/backend-administrative-division.ts ./src/models/parametric-tables-binds.ts ./src/models/backend-gdpr-consents.ts ./src/models/backend-create-new-application.ts",
    "build-post-cg": "webpack-cli --progress --config webpack.config.js",
    "build": "run-s code-generation build-post-cg",
    "build-min": "NODE_ENV=production npm run build",
    "dev": "run-s code-generation dev-post-cg",
    "dev-post-cg": "webpack serve --hot --inline --progress  --port 8090",
    "test": "jest",
    "test.README": [
      "For continuous testing use: `npm test -- --watch`",
      "Also, don't forget (during troubleshooting) to clear Jest's cache",
      "See below target."
    ],
    "clear-jest-cache": "jest --clearCache",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet",
    "lint.fix": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
